<h2>Edit where can go</h2>

<form asp-area="Specialist" asp-controller="Profile" asp-action="EditWhereCanGo" method="post">
    <div class="form-group">
        <label>Tags:</label>
        <select name="points" asp-items="@ViewBag.MeetingPoints" data-placeholder="Choose meeting points..." multiple id="points" class="form-control chosen-select"></select>
    </div>
    <button class="btn btn-success" type="submit">Save</button>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
    <script src="~/lib/chosen/chosen.jquery.min.js"></script>
    <script>
        $('#points').chosen();
    </script>
}